<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ammar.modulink">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.ModulView" />
        <activity android:name=".ui.ProfilActivty" />
        <activity android:name=".ui.DosenActivity"/>
        <activity android:name=".ui.LoginActivity" />
        <activity android:name=".ui.MainScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.ModulActivity"
            android:parentActivityName=".ui.DosenActivity">

            <meta-data
                android:name="android.support.PARENT_ACTIVTY"
                android:value=".ui.DosenActivty"/>

        </activity>
    </application>

</manifest>